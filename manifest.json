{
  "manifest_version": 2,

  "name": "JS Keycodes",
  "description": "This extension is meant to get the keycodes used in javascript",
  "version": "1.0",

  "browser_action": {
    "default_icon": "/res/js-keycode-icon.png",
    "default_popup": "/view/index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
