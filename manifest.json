{
  "manifest_version": 2,
  "name": "JS Keycodes",
  "version": "1.1",

  "description": "Easy. Quick. Clean. Get access to JavaScript keycodes with this extension.",
  "icons": {
    "16": "/res/js-keycode-icon16.png",
    "48": "/res/js-keycode-icon48.png",
    "128": "/res/js-keycode-icon128.png"
  },

  "browser_action": {
    "default_icon": "/res/js-keycode-icon16.png",
    "default_popup": "/view/index.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
